{
  "name": "qc-atlas-ui",
  "version": "3.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "eslint ./src --ext .js,.ts",
    "lint-fix": "eslint ./src --fix --ext .js,.ts",
    "gen": "npm run gen-atlas && npm run gen-patternatlas && npm run gen-nisq && npm run gen-latex",
    "gen-atlas": "ng-openapi-gen --input http://localhost:6626/atlas/v3/api-docs -o generated/api-atlas",
    "gen-patternatlas": "ng-openapi-gen --input http://localhost:1977/patternatlas/v3/api-docs -o generated/api-patternatlas",
    "gen-nisq": "ng-openapi-gen --input http://localhost:5010/nisq-analyzer/v3/api-docs -o generated/api-nisq",
    "gen-latex": "ng-openapi-gen --input http://localhost:5030/v3/api-docs -o generated/api-latex",
    "gen-qprov": "ng-openapi-gen --input http://localhost:5020/qprov/v3/api-docs -o generated/api-qprov",
    "gen-library": "ng-openapi-gen --input http://localhost:2903/openapi -o generated/api-library",
    "postgen": "prettier generated/ --write",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^13.3.11",
    "@angular/animations": "^13.4.0",
    "@angular/common": "~13.4.0",
    "@angular/cdk": "^12.2.13",
    "@angular/compiler": "~13.4.0",
    "@angular/core": "^13.4.0",
    "@angular/forms": "~13.4.0",
    "@angular/localize": "~13.4.0",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~13.4.0",
    "@angular/platform-browser-dynamic": "~13.4.0",
    "@angular/router": "~13.4.0",
    "@fortawesome/fontawesome-free": "^5.13.1",
    "@swimlane/ngx-graph": "^8.0.3",
    "animate.css": "^4.1.0",
    "chart.js": "^2.5.0",
    "deep-equal": "^2.0.3",
    "font-awesome": "^4.7.0",
    "keycloak-angular": "9.3.2",
    "keycloak-js": "^10.0.2",
    "ng2-pdf-viewer": "^6.3.2",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular/cli": "~13.3.11",
    "@angular-devkit/build-angular": "13.3.11",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
    "@angular/compiler-cli": "^13.4.0",
    "@angular/language-service": "~13.4.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "codelyzer": "^6.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^25.4.0",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "hammerjs": "^2.0.8",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-openapi-gen": "git+https://github.com/timniederhausen/ng-openapi-gen#af5c4278587c47999f3c9b48da158398736a7fed",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.6.4"
  }
}
